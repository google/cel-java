load("@com_google_protobuf//bazel:java_proto_library.bzl", "java_proto_library")
load("//:java_lite_proto_cel_library.bzl", "java_lite_proto_cel_library")

package(
    default_applicable_licenses = [
        "//:license",
    ],
    default_visibility = [
        "//visibility:public",
    ],
)

proto_library(
    name = "simple_test_proto",
    srcs = ["simple_test.proto"],
)

java_proto_library(
    name = "simple_test_java_proto",
    deps = [
        ":simple_test_proto",
    ],
)

java_lite_proto_cel_library(
    name = "simple_test_java_lite_cel_proto",
    descriptor_class_prefix = "SimpleTest",
    deps = [
        ":simple_test_proto",
    ],
)
